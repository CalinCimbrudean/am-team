@model IEnumerable<TheAMTeam.Business.Models.ArticleBussinessModel>
@{

    Layout = "/Views/Shared/_LayoutArticle.cshtml";
}


<h2>Article</h2>

@foreach (var field in Model)
{
    <input type="button" value="Add new article" onclick="location.href='@Url.Action("Add", "Article")'" />
    <div>
        @field.Title
        @Html.ActionLink("Edit", "Edit", "Article", new { id = field.ArticleId }, null)
        @Html.ActionLink("Delete", "Delete", "Article", new { id = field.ArticleId }, null)

    </div>
}










