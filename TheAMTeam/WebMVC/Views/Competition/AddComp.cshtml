@model TheAMTeam.Business.Models.CompetitionTypeModel

@{
    Layout = "~/Views/Shared/CompetitionLayout.cshtml";
}

<h1>Add Competition </h1>

@using (Html.BeginForm("AddCompetition", "Competition", FormMethod.Post))
{
    <div>
        <label>Id</label>
        @Html.TextBoxFor(x => x.CompetitionTypeId)
    </div>
    <div>
        <p>
            <label>Name</label>
            @Html.TextBoxFor(x => x.Name)
        </p>
    </div>
    <div>
        <input type="submit" value="Submit form" /> <button onclick="goBack()">Go Back</button>
    </div>
}

<script src="~/assets/js/script.js"></script>